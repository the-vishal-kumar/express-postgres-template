{
  "name": "nodejs-postgres-template",
  "version": "1.0.0",
  "description": "CRUD application using NodeJs with Express framework, PostgreSQL as database, Sequeslize as ORM. Application uses rollbar for error tracking. Also contains Postman APIs collection",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "db:migrate": "sequelize db:migrate",
    "db:unmigrate": "sequelize db:migrate:undo"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/the-vishal-kumar/nodejs-postgres-template"
  },
  "keywords": [
    "nodejs",
    "express",
    "postgresql",
    "sequelize",
    "rollbar",
    "postman"
  ],
  "author": "Vishal Kumar",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-body-parser-json-error": "^0.1.0",
    "express-validator": "^6.6.1",
    "path": "^0.12.7",
    "pg": "^8.3.0",
    "rollbar": "^2.19.2",
    "sequelize": "^6.3.4",
    "sequelize-cli": "^6.2.0",
    "sequelize-log-syntax-colors": "^2.0.0",
    "sql-formatter": "^2.3.3",
    "url": "^0.11.0"
  }
}